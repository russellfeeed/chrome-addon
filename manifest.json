{
  "manifest_version": 3,
  "name": "Simple Chrome Extension",
  "version": "1.0.0",
  "description": "A simple Chrome extension for testing private deployment",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
